{
    "compilerOptions": {
        "module": "es2022",
        "target": "es2021",
        "lib": ["es2021"],
        "strict": true,
        "moduleResolution": "node",
        "types": ["@cloudflare/workers-types"],
        "resolveJsonModule": true,
        "allowJs": true,
        "checkJs": true,
        "noEmit": true,
        "isolatedModules": true,
        "allowSyntheticDefaultImports": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true
    }
}
